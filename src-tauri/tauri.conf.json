{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "noscreenshot",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "create": true,
        "minimize": true,
        "close": true,
        "unminimize": true
      },
      "dialog": {
        "message": true
      },
      "protocol": {
        "asset": true,
        "assetScope": ["**"]
      },
      "globalShortcut": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.noscreenshot",
      "targets": "all"
    },
    "security": {
      "csp": ""
    },
    "updater": {
      "active": false
    },
    "windows": []
  }
}
